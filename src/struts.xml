<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="sistemaescolar" namespace="/" extends="struts-default">
        <action name="crearcircular" class="sistemaescolar.action.CircularAction" method="create">
            <result name="success">/resultado.jsp</result>
        </action>

        <action name="getcircularbyid" class="sistemaescolar.action.CircularAction" method="getById">
            <result name="success">/getcircularbyid.jsp</result>
        </action>

        <action name="getcircularall" class="sistemaescolar.action.CircularAction" method="getAll">
            <result name="success">/getcircularall.jsp</result>
        </action>
    </package>
</struts>