<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="sistemaescolar.Alumno" table="Alumno" lazy="false">
		<id name="idAlumno" column="idAlumno" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="nombreAlumno" column="nombreAlumno" type="string" length="100" not-null="false" lazy="false"/>
		<property name="passwordAlumno" column="passwordAlumno" type="string" length="10" not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
