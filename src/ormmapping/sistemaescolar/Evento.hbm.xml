<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="sistemaescolar.model.Evento" table="Evento" lazy="false">
        <id name="idCircular" column="idCircular" type="integer" unsaved-value="0">
            <generator class="native">
            </generator>
        </id>
        <property name="titulo" column="titulo" type="string" length="100" not-null="false" lazy="false"/>
        <property name="fecha" column="fecha" type="string" length="100" not-null="false" lazy="false"/>
        <property name="remitente" column="remitente" type="string" length="100" not-null="false" lazy="false"/>
        <property name="contenido" column="contenido" type="string" length="100" not-null="false" lazy="false"/>
    </class>
</hibernate-mapping>
